<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >

<hibernate-mapping package="cg.genealogie.orm">
	<class name="Nomme" table="nomme">
		<composite-id>
			<key-many-to-one
				class="Nom"
				column="nom"
				name="Nom"
			 />
			<key-many-to-one
				class="Personne"
				column="idPersonne"
				name="IdPersonne"
			 />
		</composite-id>
	</class>
</hibernate-mapping>
